lexer grammar VajramLexer;

AND : 'and' ;
OR : 'or' ;
NOT : 'not' ;
EQ : '=' ;
COMMA : ',' ;
SEMI : ';' ;
LPAREN : '(' ;
RPAREN : ')' ;
LCURLY : '{' ;
RCURLY : '}' ;
VAJRAM : 'vajram' ;
DEP    : 'dep' ;
OUT    : 'out' ;
RETURN : 'return' ;
PERMITS: 'permits' ;
APP    : 'app' ;
CLIENT : 'client' ;
MANDATORY : 'mandatory' ;

INT : [0-9]+ ;
TRUE : 'true' ;
FALSE : 'false' ;
ID: [a-zA-Z_][a-zA-Z_0-9]* ;
WS: [ \t\n\r\f]+ -> skip ;
